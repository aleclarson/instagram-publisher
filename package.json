  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@types/request-promise-native": "^1.0.18",
    "@types/tough-cookie": "^4.0.2",
    "sharp": "^0.32.4",
    "tsup": "^7.2.0",
    "typescript": "^4.6.4",
    "vitest": "^0.34.1"
  },
  "dependencies": {
    "ffprobe": "^1.1.2",
    "ffprobe-static": "^3.0.0",
    "image-size": "^1.0.1",
    "request-promise-native": "^1.0.9",
    "tough-cookie": "^4.0.0"
  }
