{
  "name": "instagram-publisher",
  "author": "Yuvraj Seegolam",
  "version": "2.3.0",
  "license": "MIT",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "repository": "aleclarson/instagram-publisher",
  "description": "Publish Instagram Images, Image Slideshows, Video Reels & Stories via NodeJS",
  "keywords": [
    "instagram",
    "slideshow",
    "ig",
    "api",
    "web",
    "photos",
    "videos",
    "reels",
    "jpg",
    "mp4"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "tsup --watch --dts --format cjs,esm --sourcemap src/index.ts",
    "build": "tsup --dts --format cjs,esm src/index.ts"
