{
    "version": "2.2.0",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": "yuvraj108c/instagram-publisher",
    "description": "Publish Instagram Images, Image Slideshows, Video Reels & Stories via NodeJS",
    "keywords": [
        "instagram",
        "slideshow",
        "ig",
        "api",
        "web",
        "photos",
        "videos",
        "reels",
        "jpg",
        "mp4"
    ],
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "jest": {
        "maxWorkers": 1,
        "testTimeout": 30000
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "name": "instagram-publisher",
    "author": "Yuvraj Seegolam",
    "module": "dist/instagram-publisher.esm.js",
    "size-limit": [
        {
            "path": "dist/instagram-publisher.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/instagram-publisher.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^7.0.8",
        "@types/request-promise-native": "^1.0.18",
        "@types/tough-cookie": "^4.0.2",
        "husky": "^7.0.4",
        "sharp": "^0.30.4",
        "size-limit": "^7.0.8",
        "tsdx": "^0.14.1",
        "tslib": "^2.4.0",
        "typescript": "^4.6.4"
    },
    "dependencies": {
        "ffprobe": "^1.1.2",
        "ffprobe-static": "^3.0.0",
        "image-size": "^1.0.1",
        "request-promise-native": "^1.0.9",
        "tough-cookie": "^4.0.0"
    }
}